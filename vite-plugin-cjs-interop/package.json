{
	"name": "vite-plugin-cjs-interop",
	"version": "0.0.6",
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/*.d.ts"
			]
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepack": "rm -rf dist && pnpm build",
		"test": "pnpm test:typecheck && pnpm test:lint && pnpm test:unit && pnpm test:package",
		"test:unit": "vitest run",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
		"test:package": "publint"
	},
	"description": "Vite plugin to unwrap default imports from CJS dependencies during SSR",
	"license": "MIT",
	"repository": "https://github.com/cyco130/vite-plugin-cjs-interop",
	"keywords": [
		"vite-plugin",
		"cjs",
		"interop",
		"ssr"
	],
	"devDependencies": {
		"@cyco130/eslint-config": "^2.1.2",
		"@types/minimatch": "^5.1.2",
		"@types/node": "^18.11.9",
		"eslint": "^8.27.0",
		"publint": "^0.1.3",
		"tsup": "^6.5.0",
		"typescript": "^4.8.4",
		"vite": "^3.2.3",
		"vitest": "^0.25.2"
	},
	"dependencies": {
		"acorn": "^8.8.1",
		"estree-walker": "^3.0.1",
		"magic-string": "^0.26.7",
		"minimatch": "^5.1.0"
	}
}
